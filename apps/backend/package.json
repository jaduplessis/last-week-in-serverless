{
  "name": "backend",
  "version": "0.1.0",
  "bin": {
    "backend": "bin/backend.js"
  },
  "scripts": {
    "bootstrap": "cdk bootstrap",
    "deploy": "cdk deploy",
    "destroy": "cdk destroy --force",
    "test": "vitest run --coverage --passWithNoTests",
    "test-inte": "vitest run --coverage --passWithNoTests -c vitest.test-inte.config.ts",
